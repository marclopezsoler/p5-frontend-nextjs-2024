generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

// Products of the store
model Product {
  id             Int      @id @default(autoincrement())
  name           String
  price          Float
  description    String
  color          String
  seller         String
  category       String
  dateOfCreation DateTime @default(now()) 
}
